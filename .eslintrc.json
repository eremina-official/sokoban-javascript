{
  "env": {
      "browser": true,
      "es2021": true
  },
  "parserOptions": {
      "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error", {
      "endOfLine": "auto",
      "semi": true
    }]
  },
  "plugins": ["prettier"],
  "extends": [
    "plugin:prettier/recommended"
  ],
  "globals": {
    "module": true,
    "process": true
  }
}
